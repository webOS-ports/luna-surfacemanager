{
    \"exeName\":\"$$WEBOS_INSTALL_BINS/surface-manager\",
    \"type\": \"privileged\",
    \"allowedNames\": [
        \"com.webos.surfacemanager*\", 
        \"org.webosports.notifications\"
    ],
    \"permissions\": [
        {
            \"service\": \"com.webos.surfacemanager*\",
            \"outbound\": [
                \"com.lge.*\",
                \"com.palm.*\",
                \"com.webos.*\",
                \"org.webosports.*\"
            ]
        },
        {
            \"service\": \"org.webosports.notifications\",
            \"outbound\": [
                \"com.lge.*\",
                \"com.palm.*\",
                \"com.webos.*\",
                \"org.webosports.*\"
            ]
        }
    ]
}
